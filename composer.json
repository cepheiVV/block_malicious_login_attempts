{
  "name": "itsc/block-malicious-login-attempts",
  "type": "typo3-cms-extension",
  "description": "Blocks IP from accessing the website when multiple login attempts fail",
  "keywords": [
    "TYPO3 CMS extension",
    "malicious login attempts",
    "block backend login"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "authors": [
    {
      "name": "Patrick Crausaz",
      "homepage": "https://its-crausaz.ch/"
    }
  ],
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4 || ^11.5"
  },
  "autoload": {
    "psr-4": {
      "ITSC\\BlockMaliciousLoginAttempts\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "block_malicious_login_attempts"
    }
  }
}
